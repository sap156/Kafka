{
    "name": "mysql-source",
    "config": {
        "connector.class": "io.confluent.connect.jdbc.JdbcSourceConnector",
        "connection.url": "jdbc:mysql://docker.for.mac.localhost:3306/kafkadb?enabledTLSProtocols=TLSv1.2&verifyServerCertificate=false&useSSL=false&requireSSL=false",
        "connection.user": "root",
        "connection.password": "******",
        "database.timezone": "UTC",
        "table.whitelist": "test_data_0",
        "mode": "timestamp",
        "timestamp.column.name": "timestamp",
        "timestamp.initial": "-1",
        "table.types": "TABLE",
        "table.poll.interval.ms": "6000",
        "topic.prefix": "mysql",
        "validate.non.null": "false",
        "tasks.max": "1",
        "poll.interval.ms": "1000",
        "batch.max.rows": "3000",
        "key.converter": "org.apache.kafka.connect.json.JsonConverter",
        "key.converter.schemas.enable": "false",
        "value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "value.converter.schemas.enable": "false"
    }
}
